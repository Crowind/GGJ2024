// Input:20
	vec2 n_out20p0 = UV;


// VectorOp:17
	vec2 n_out17p0 = n_out23p0 + n_out20p0;


	vec4 n_out15p0;
// Texture2D:15
	n_out15p0 = texture(Mask, n_out17p0);
	float n_out15p4 = n_out15p0.a;


// FloatFunc:25
	float n_out25p0 = 1.0 - n_out15p4;


// Mix:22
	vec4 n_out22p0 = mix(n_out13p0, vec4(n_out21p0, 0.0), vec4(n_out25p0));


// Output:0
